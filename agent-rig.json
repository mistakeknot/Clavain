{
  "name": "clavain",
  "version": "0.6.116",
  "description": "Recursively self-improving multi-agent rig — brainstorm to ship. 4 agents, 45 commands, 15 skills, 1 MCP server. Companions: interphase, interline, interslack, interform, intercraft, interdev.",
  "author": "mistakeknot",
  "license": "MIT",
  "repository": "mistakeknot/Clavain",
  "keywords": [
    "engineering-discipline",
    "code-review",
    "workflow-automation",
    "tdd",
    "debugging",
    "planning",
    "agents",
    "general-purpose",
    "cross-ai-review",
    "oracle",
    "council"
  ],
  "plugins": {
    "core": {
      "source": "clavain@interagency-marketplace",
      "description": "The core Clavain multi-agent rig"
    },
    "required": [
      {
        "source": "context7@claude-plugins-official",
        "description": "Runtime doc fetching via MCP"
      },
      {
        "source": "explanatory-output-style@claude-plugins-official",
        "description": "Educational output formatting"
      }
    ],
    "recommended": [
      {
        "source": "interdoc@interagency-marketplace",
        "description": "AGENTS.md generation"
      },
      {
        "source": "interflux@interagency-marketplace",
        "description": "Multi-agent review engine (flux-drive)"
      },
      {
        "source": "interphase@interagency-marketplace",
        "description": "Phase tracking, gates, and discovery",
        "doctorCheck": {
          "label": "Beads Lifecycle Companion",
          "probe": "hooks/lib-gates.sh",
          "notInstalledMsg": "phase tracking disabled"
        }
      },
      {
        "source": "interline@interagency-marketplace",
        "description": "Dynamic statusline renderer",
        "doctorCheck": {
          "label": "Statusline Companion",
          "probe": "scripts/statusline.sh",
          "notInstalledMsg": "statusline rendering unavailable"
        }
      },
      {
        "source": "interpath@interagency-marketplace",
        "description": "Product artifact generation",
        "doctorCheck": {
          "label": "Artifact Generation Companion",
          "probe": "scripts/interpath.sh",
          "notInstalledMsg": "product artifact generation unavailable"
        }
      },
      {
        "source": "interwatch@interagency-marketplace",
        "description": "Doc freshness monitoring",
        "doctorCheck": {
          "label": "Doc Freshness Companion",
          "probe": "scripts/interwatch.sh",
          "notInstalledMsg": "doc drift detection unavailable"
        }
      },
      {
        "source": "interlock@interagency-marketplace",
        "description": "Multi-agent file coordination"
      },
      {
        "source": "intercheck@interagency-marketplace",
        "description": "Code quality guards and session health"
      },
      {
        "source": "tldr-swinton@interagency-marketplace",
        "description": "Token-efficient code context (MCP)"
      },
      {
        "source": "tool-time@interagency-marketplace",
        "description": "Tool usage analytics"
      },
      {
        "source": "interslack@interagency-marketplace",
        "description": "Slack integration"
      },
      {
        "source": "interform@interagency-marketplace",
        "description": "Design patterns and visual quality"
      },
      {
        "source": "intercraft@interagency-marketplace",
        "description": "Agent-native architecture patterns"
      },
      {
        "source": "interdev@interagency-marketplace",
        "description": "MCP CLI and developer tooling"
      },
      {
        "source": "agent-sdk-dev@claude-plugins-official",
        "description": "Agent SDK development tools"
      },
      {
        "source": "plugin-dev@claude-plugins-official",
        "description": "Plugin development tools"
      },
      {
        "source": "serena@claude-plugins-official",
        "description": "Semantic coding tools"
      },
      {
        "source": "security-guidance@claude-plugins-official",
        "description": "Security best practices"
      },
      {
        "source": "intertest@interagency-marketplace",
        "description": "Engineering quality disciplines (TDD, debugging, verification)"
      },
      {
        "source": "interpeer@interagency-marketplace",
        "description": "Cross-AI peer review (Claude/Codex/Oracle)"
      },
      {
        "source": "intersynth@interagency-marketplace",
        "description": "Multi-agent synthesis engine (verdict aggregation)"
      },
      {
        "source": "intermap@interagency-marketplace",
        "description": "Project-level code mapping and architecture analysis (MCP)"
      },
      {
        "source": "internext@interagency-marketplace",
        "description": "Work prioritization and tradeoff analysis"
      },
      {
        "source": "intermem@interagency-marketplace",
        "description": "Memory synthesis — graduates auto-memory to AGENTS.md/CLAUDE.md"
      }
    ],
    "optional": [
      {
        "source": "interfluence@interagency-marketplace",
        "description": "Voice profile and style adaptation"
      },
      {
        "source": "interject@interagency-marketplace",
        "description": "Ambient discovery and research engine (MCP)"
      },
      {
        "source": "interstat@interagency-marketplace",
        "description": "Token efficiency benchmarking"
      },
      {
        "source": "interkasten@interagency-marketplace",
        "description": "Notion sync and documentation"
      },
      {
        "source": "interlens@interagency-marketplace",
        "description": "Cognitive augmentation lenses (MCP)"
      },
      {
        "source": "intersearch@interagency-marketplace",
        "description": "Shared embedding and Exa search library"
      },
      {
        "source": "interserve@interagency-marketplace",
        "description": "Codex spark classifier and context compression (MCP)"
      },
      {
        "source": "interpub@interagency-marketplace",
        "description": "Plugin publishing automation"
      },
      {
        "source": "tuivision@interagency-marketplace",
        "description": "TUI automation and visual testing (MCP)"
      },
      {
        "source": "intermux@interagency-marketplace",
        "description": "Agent activity visibility and tmux monitoring (MCP)"
      },
      {
        "source": "interleave@interagency-marketplace",
        "description": "Deterministic skeleton + LLM islands pattern"
      },
      {
        "source": "interlearn@interagency-marketplace",
        "description": "Cross-repo institutional knowledge index"
      },
      {
        "source": "intercache@interagency-marketplace",
        "description": "Cross-session semantic cache (MCP)"
      },
      {
        "source": "interchart@interagency-marketplace",
        "description": "Interactive ecosystem diagram generator"
      }
    ],
    "infrastructure": [
      {
        "source": "gopls-lsp@claude-plugins-official",
        "description": "Go language server"
      },
      {
        "source": "pyright-lsp@claude-plugins-official",
        "description": "Python language server"
      },
      {
        "source": "typescript-lsp@claude-plugins-official",
        "description": "TypeScript language server"
      },
      {
        "source": "rust-analyzer-lsp@claude-plugins-official",
        "description": "Rust language server"
      }
    ],
    "conflicts": [
      {
        "source": "code-review@claude-plugins-official",
        "reason": "Duplicates Clavain review agents"
      },
      {
        "source": "pr-review-toolkit@claude-plugins-official",
        "reason": "Duplicates Clavain PR review"
      },
      {
        "source": "code-simplifier@claude-plugins-official",
        "reason": "Duplicates simplicity reviewer"
      },
      {
        "source": "commit-commands@claude-plugins-official",
        "reason": "Duplicates commit workflow"
      },
      {
        "source": "feature-dev@claude-plugins-official",
        "reason": "Duplicates feature dev workflow"
      },
      {
        "source": "claude-md-management@claude-plugins-official",
        "reason": "Conflicts with doc management"
      },
      {
        "source": "frontend-design@claude-plugins-official",
        "reason": "Conflicts with design agents"
      },
      {
        "source": "hookify@claude-plugins-official",
        "reason": "Conflicts with hook management"
      },
      {
        "source": "superpowers@superpowers-marketplace",
        "reason": "Clavain is the successor to superpowers"
      },
      {
        "source": "compound-engineering@every-marketplace",
        "reason": "Clavain is the successor to compound-engineering"
      }
    ]
  },
  "mcpServers": {
    "context7": {
      "type": "http",
      "url": "https://mcp.context7.com/mcp",
      "description": "Runtime documentation fetching"
    },
    "qmd": {
      "type": "stdio",
      "command": "qmd",
      "args": [
        "mcp"
      ],
      "description": "Local semantic search engine"
    }
  },
  "tools": [
    {
      "name": "oracle",
      "install": "npm install -g @steipete/oracle",
      "check": "command -v oracle",
      "optional": true,
      "description": "Cross-AI review via GPT-5.2 Pro"
    },
    {
      "name": "codex",
      "install": "npm install -g @openai/codex",
      "check": "command -v codex",
      "optional": true,
      "description": "OpenAI's coding agent for parallel dispatch"
    },
    {
      "name": "beads",
      "install": "npm install -g @steveyegge/beads",
      "check": "command -v bd",
      "optional": true,
      "description": "Git-native issue tracking"
    },
    {
      "name": "qmd",
      "install": "go install github.com/tobi/qmd@latest",
      "check": "command -v qmd",
      "optional": true,
      "description": "Semantic search across documentation"
    }
  ],
  "environment": {
    "DISPLAY": ":99",
    "CHROME_PATH": "/usr/local/bin/google-chrome-wrapper"
  },
  "behavioral": {
    "claude-md": {
      "source": "config/CLAUDE.md"
    }
  },
  "postInstall": {
    "message": [
      "Try: /sprint add user export feature",
      "Run /help to see all 32 commands, or /doctor to verify your setup."
    ]
  },
  "platforms": {
    "claude-code": {
      "marketplaces": [
        {
          "name": "interagency-marketplace",
          "repo": "mistakeknot/interagency-marketplace"
        },
        {
          "name": "claude-plugins-official",
          "repo": "anthropics/claude-plugins-official"
        }
      ]
    },
    "codex": {
      "installScript": "scripts/install-codex.sh",
      "skillsDir": "~/.codex/skills/clavain"
    }
  }
}
